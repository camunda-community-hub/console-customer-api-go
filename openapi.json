  "paths": {
    "/clusters": {
      "get": {
        "operationId": "GetClusters",
        "parameters": [],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Cluster"
                  },
                  "type": "array"
                }
              }
            },
            "description": "A list of all your Camunda clusters."
          }
        },
        "security": [
          {
            "bearer": []
          }
        ]
      },
      "post": {
        "operationId": "CreateCluster",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateClusterBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "clusterId": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "clusterId"
                  ],
                  "type": "object"
                }
              }
            },
            "description": "The clusterUuid of your new cluster."
          },
          "400": {
            "description": "Name is too long, invalid clusterPlanType, invalid region, no reservations left, invalid channel, or invalid generation"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
